<Brushes>
    <Brush Name="TorInventoryMagicItemBrush" Font="FiraSansExtraCondensed-Regular" TextHorizontalAlignment="Center">
        <Styles>
            <Style Name="Default" FontColor="#0095A4FF" TextBlur="0" TextOutlineAmount="0" TextGlowRadius="0" FontSize="16" />
        </Styles>
    </Brush>
    <Brush Name="TorSpellBookBrush" Font="FiraSansExtraCondensed-Regular" TextHorizontalAlignment="Center">
        <Styles>
            <Style Name="Default" FontColor="#000000FF" TextBlur="0" TextOutlineAmount="0" TextGlowRadius="0" FontSize="16" />
        </Styles>
    </Brush>
    <Brush Name="TorSpellBookMarkBrush">
        <Layers>
            <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9" />
        </Layers>
        <Styles>
            <Style Name="Default">
                <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9" />
            </Style>
            <Style Name="Hovered">
                <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9" Color="#404040FF"/>
            </Style>
            <Style Name="Pressed">
                <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9"/>
            </Style>
            <Style Name="Selected">
                <BrushLayer Name="Default" Sprite="BlankWhiteSquare_9"/>
            </Style>
        </Styles>
    </Brush>
    <Brush Name="TorSpellItemBrush">
        <Layers>
            <BrushLayer Name="Default" />
        </Layers>
        <Styles>
            <Style Name="Default">
                <BrushLayer Name="Default" />
            </Style>
            <Style Name="Hovered">
                <BrushLayer Name="Default" />
            </Style>
            <Style Name="Pressed">
                <BrushLayer Name="Default" />
            </Style>
            <Style Name="Disabled">
                <BrushLayer Name="Default" ColorFactor="0.4" AlphaFactor="0.6"/>
            </Style>
        </Styles>
    </Brush>
    <Brush Name="TorSpecialMoveBrush">
        <Layers>
            <BrushLayer Name="EmptyFill" Sprite="BlankWhiteSquare" ColorFactor="1" Color="#404040FF" />
            <BrushLayer Name="DefaultFill" Sprite="BlankWhiteSquare" ColorFactor="1" Color="#FF2222FF" />
            <BrushLayer Name="ChangeFill" Sprite="BlankWhiteSquare" ColorFactor="1" Color="#FFAAAAFF" />
        </Layers>
        <Styles>
            <Style Name="Default" />
        </Styles>
    </Brush>
</Brushes>